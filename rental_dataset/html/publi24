        $(document).ready(function () {
             $(".partialContent1").each(function(index, item) {
		        var url = $(item).data("url");
		        if (url && url.length > 0 ) {
                    $(item).load(url);
		        }
	        });
            $(".partialContent2").each(function(index, item) {
		        var url = $(item).data("url");
		        if (url && url.length > 0 ) {
                    $(item).load(url, function () {
                        if ($(".partialContent2").html().indexOf("online") != -1) {
                            $(".detail-right .panel.user a.userimg").addClass('is-online');
                            $(".detail-right .panel.user a.userimg").append('<span></span>');
                        }
                    });
		        }
	        });
        $('#sendmessage').click(function () {
            $('#sendmessage').find("span, i").css("display", "inline-block");

            if (!UserLoggedIn) {
                $('#sendmessage').find(".fa-circle-o-notch").css("display", "none");
                return;
            }

            if ($('#messagetext').val() == '') {
                $('#messagetext').focus();
                $('#sendmessage').find(".fa-circle-o-notch").css("display", "none");
                return;
            }
            var attachments = [];
            $('#attachments .row').each(function (index) {
                attachments.push({ FileName: $(this).data('filename'), FileUrl: $(this).data('fileurl') });
            });
            var messageText = {
                messageType: 'text',
                messageContent: $('#messagetext').val(),
                attachments: attachments
            };
            var data = {
                ObjectId: '92399eh4h6h37ffgeeiie306220i4i3i',
                MainCategory: 'Imobiliare',
                ReciverSource: '',
                MessageText: JSON.stringify(messageText),
                ObjectTitle: JSON.stringify('Apartament cu 2 camere + curte, situat in zona Sagului')
            };

            $.ajax({
                type: "POST",
                url: '/Inbox/SendInbox',
                data: data,
                success: function (data) {
                   if (data == 'sent') {
                            $('#sendoffersuccess').show().delay(1000).fadeOut(1000);
                        }
                        else {
                            if(data == 'vip')
                             $('#vip_trigger').show();
                            else
                                $('#sendoffererror').show().delay(1000).fadeOut(1000);
                        }
                    $('#messagetext').val('');
                    $('#attachments').html('');
                    $('#sendmessage').find("span, i").css("display", "inline-block");
                    $('#sendmessage').find(".fa-circle-o-notch").css("display", "none");
                },
                error: function () {
                    $('#messagetext').focus();
                }
            });
        });

        $('#sendoffer').click(function () {
            if ($('#offervalue').val() == '') {
                $('#offervalue').focus();
                return;
            }
            var messageText = {
                messageType: 'offer',
                messageContent: $('#offervalue').val() + ' ' + 'EUR'
            };
            var data = {
                ObjectId: '92399eh4h6h37ffgeeiie306220i4i3i',
                MainCategory: 'Imobiliare',
                ReciverSource: '',
                MessageText: JSON.stringify(messageText),
                ObjectTitle: JSON.stringify('Apartament cu 2 camere + curte, situat in zona Sagului')
            };

            $.ajax({
                type: "POST",
                url: '/Inbox/SendInbox',
                data: data,
                success: function (data) {
                     if (data == 'sent') {
                            $('#sendoffersuccess').show().delay(1000).fadeOut(1000);
                        }
                        else {
                            if(data == 'vip')
                             $('#vip_trigger').show();
                            else
                                $('#sendoffererror').show().delay(1000).fadeOut(1000);
                        }
                    $(".makeoffer-modal").delay(1000).fadeOut(1000);
                    $(".loader-inline").find("span, i").css("display", "inline-block");
                    $(".loader-inline").find(".fa-circle-o-notch").css("display", "none");
                },
                error: function () {
                    $(".makeoffer-modal").delay(1800).fadeOut(1000);
                }
            });
        });
    });
    </script>
    <!--Attachments-->
    <script>
        $(function () {
            $('#paperclip').click(function () {
                if ($('#attachments row').length < 5) {
                    $('#attachment').click();
                }
                else {
                    alert('Maximum 5 files!');
                }
            });
            $("input:file").change(function () {
                if (window.FormData !== undefined) {
                    var fileUpload = $("#attachment").get(0);
                    var files = fileUpload.files;

                    var fileData = new FormData();

                    fileData.append(files[0].name, files[0]);
                    $.ajax({
                        url: '/Inbox/UploadAttachment',
                        type: "POST",
                        contentType: false, // Not to set any content header
                        processData: false, // Not to process data
                        data: fileData,
                        success: function (result) {
                            if (result) {
                                $('#attachments').append('<li class="row" data-fileurl="' + result.FileUrl + '" data-filename="' + result.FileName + '">' + result.FileName + '<a class="right errorcolor" onclick="javascript:RemoveAttachment(this)">Sterge</a></li>');
                            }
                        }
                    });
                } else {
                    alert("FormData is not supported.");
                }
            });
        });
        function RemoveAttachment(item) {
            $(item).parent().remove();
        }

    </script>
